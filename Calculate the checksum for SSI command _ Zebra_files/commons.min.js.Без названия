            $("input[name='./hideInNav']").change(function() {
			    if($(this).is(":checked")){
                      $("input[name='./hideBreadcrumb']").attr('checked', false);
                    }    
                });
                
            $("input[name='./hideBreadcrumb']").change(function() {
                if($(this).is(":checked")){
                    $("input[name='./hideInNav']").attr('checked', false);
                }    
            });
    });

});
